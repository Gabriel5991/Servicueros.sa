@model IEnumerable<ServicuerosSA.Models.Lote>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Codigolote)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Numerodepieles)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Fechaingreso)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Observaciones)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Personal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoPieles)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Codigolote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numerodepieles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fechaingreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personal.PersonalId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoPieles.TipoPielId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LoteId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LoteId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LoteId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
